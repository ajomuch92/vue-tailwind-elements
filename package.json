{
  "name": "vue-tailwind-elements",
  "version": "1.4.2",
  "description": "A library of components based on Tailwin Elements and Tailwind",
  "main": "dist/vue-tailwind-elements.ssr.js",
  "browser": "dist/vue-tailwind-elements.esm.js",
  "module": "dist/vue-tailwind-elements.esm.js",
  "unpkg": "dist/vue-tailwind-elements.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "src/**/*.css",
    "src/**/*.js"
  ],
  "keywords": [
    "tailwind",
    "vuejs",
    "vuetailwind",
    "tailwind-elements",
    "vue-tailwind-elements",
    "library",
    "components"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajomuch92/vue-tailwind-elements.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build-prod": "vue-cli-service build dev/",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "autoprefixer": "^9.8.8",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^7.0.39",
    "prismjs": "^1.25.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "vue": "^2.6.14",
    "vue-prism": "^1.0.5",
    "vue-router": "^3.5.3",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "style": "src/tailwind-forms.min.css"
}
